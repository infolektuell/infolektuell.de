---
import type { MarkdownLayoutProps } from 'astro'
import { type BaseData, baseData } from '../lib/schemas/base'
import BaseLayout from './BaseLayout.astro'
export type Props = MarkdownLayoutProps<BaseData>
// eslint-disable-next-line @typescript-eslint/no-unnecessary-type-assertion
const fm = baseData.parse((Astro.props as Props).frontmatter)
---

<BaseLayout {...fm}>
  <slot />
</BaseLayout>
